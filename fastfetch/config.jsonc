    {
      "type": "swap",
      "key": "│ ├󰓡",
      "keyColor": "white"
    },
    {
      "type": "uptime",
      "key": "│ ├󰅐",
      "keyColor": "white"
    },
    {
      "type": "player",
      "key": "│ ├󰥠",
      "keyColor": "white"
    },
    {
      "type": "media",
      "key": "│ └󰝚",
      "keyColor": "white"
    },
    {
      "type": "custom",
      "format": "└{$1}{$1}┘",
      "outputColor": "white"
    },
    "break"
